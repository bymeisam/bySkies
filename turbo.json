{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"],
      "cache": false,
      "env": ["OPENWEATHER_API_KEY", "NODE_ENV"]
    },
    "lint": {},
    "check-types": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "env": ["OPENWEATHER_API_KEY", "NODE_ENV"]
    }
  }
}
